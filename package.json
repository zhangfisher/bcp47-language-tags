{
	"name": "bcp47-language-tags",
	"version": "1.0.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"description": "list BCP 47 language tags and types",
	"homepage": "https://github.com/zhangfisher/bcp47-language-tags",
	"repository": {
        "type": "git",
        "url": "git+https://github.com/zhangfisher/bcp47-language-tags"
    },
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"exports": {
		".": {			
			"types": "./dist/zh.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./en": {
			"types": "./dist/zh.d.ts",
			"require": "./dist/en.js",
			"import": "./dist/en.mjs"
		},
		"./zh": {			
			"types": "./dist/zh.d.ts",
			"require": "./dist/zh.js",
			"import": "./dist/zh.mjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"changeset": "changeset",
		"release": "changeset version && changeset publish"
	},
	"packageManager": "pnpm@9.0.0",
	"devDependencies": {
		"@changesets/cli": "^2.27.11",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
